<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EditarPessoa</servlet-name>
        <servlet-class>com.servlet.EditarPessoa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarActor</servlet-name>
        <servlet-class>com.servlet.EditarActor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarPessoa</servlet-name>
        <servlet-class>com.servlet.EliminarPessoa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarFilmes</servlet-name>
        <servlet-class>com.servlet.CadastrarFilme</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarSocio</servlet-name>
        <servlet-class>com.servlet.CadastrarSocio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarPessoa</servlet-name>
        <servlet-class>com.servlet.CadastrarPessoa</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditarPessoa</servlet-name>
        <url-pattern>/EditarPessoa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarActor</servlet-name>
        <url-pattern>/EditarActor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarPessoa</servlet-name>
        <url-pattern>/EliminarPessoa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarFilmes</servlet-name>
        <url-pattern>/CadastrarFilmes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarSocio</servlet-name>
        <url-pattern>/CadastrarSocio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarPessoa</servlet-name>
        <url-pattern>/CadastrarPessoa</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
